<!DOCTYPE html>
<html lang="en">
<head>
    <title>Sign Up</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" href="images/ExamSpur_Im/favicon.png" type="image/x-icon" />
    <!--===============================================================================================-->
    <link href="../../plugins/lovendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link href="../../plugins/lofonts/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link href="../../plugins/lofonts/iconic/css/material-design-iconic-font.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link href="../../plugins/lovendor/animate/animate.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link href="../../plugins/lovendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link href="../../plugins/lovendor/animsition/css/animsition.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link href="../../plugins/lovendor/select2/select2.min.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link href="../../plugins/lovendor/daterangepicker/daterangepicker.css" rel="stylesheet" />
    <!--===============================================================================================-->
    <link href="../../styles/locss/main.css" rel="stylesheet" />
    <link href="../../styles/locss/util.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=ABeeZee" rel="stylesheet">

    <!--===============================================================================================-->
    <style>
        .wrap-login100 {
            width: 500px;
            background: rgba(0, 0, 0, 0.5);
            border-radius: 10px;
            overflow: hidden;
        }

        h1, h2, h3, h4, h5, h6, p, li, span, input, div, button, a, em {
            font-family: 'ABeeZee', sans-serif !important;
        }

        a {
            color: #9b9b9b;
        }

        .label-input100 {
            color: #9b9b9b;
        }

        .login100-form-title {
            font-family: Poppins-Bold;
            color: #fff;
        }

        .txt1 {
            color: #fff;
        }

        button {
            outline: none !important;
            border: none;
        }

        .input100 {
            color: #fff !important;
        }

        .txt2 {
            font-family: Poppins-Regular;
            font-size: 20px;
            line-height: 1.5;
            color: #9b9b9b;
            text-transform: uppercase;
        }

        a:hover {
            text-decoration: none;
            color: #a20a0a;
        }

        .p-b-54 {
            padding-bottom: 15px;
        }

        .p-t-65 {
            padding-top: 15px;
        }

        .p-r-55 {
            padding-right: 35px;
        }

        .p-l-55 {
            padding-left: 35px;
        }

        .p-t-155 {
            padding-top: 30px;
        }

        .p-t-54 {
            padding-top: 30px;
        }

        .p-b-49 {
            padding-bottom: 0px;
        }

        .input100 {
            height: 25px !important;
        }
    </style>
</head>
<body>
    <div class="limiter">
        <div class="container-login100" style="background-image: url('../../Images/suimages/SignUpN.jpeg');">

            <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
                <div class="login100-form validate-form">
                    <span class="login100-form-title p-b-49">
                        Sign Up
                    </span>

                    <div class="wrap-input100 validate-input" data-validate="Your First Name is required">
                        <span class="label-input100"></span>
                        <input class="input100" type="text" name="name" placeholder="Name" id="fname">
                        <span class="focus-input100" data-symbol="&#xf206;"></span>
                    </div>

                    <div class="container-login100-form-btn" style="margin-top:30px">
                        <div class="row text-center">
                            <div class="col-md-6">
                                <div class="wrap-login100-form-btn">
                                    <div class="login100-form-bgbtn"></div>
                                    <button class="login100-form-btn" id="btnregisterJamb" type="button"examid="1000">
                                        JAMB
                                    </button>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="wrap-login100-form-btn">
                                    <div class="login100-form-bgbtn"></div>
                                    <button class="login100-form-btn" id="btnregisterWaec" type="button" examid="1002">
                                        WAEC
                                    </button>
                                </div>
                            </div>

                        </div>
                       
                       
                    </div>

                </div>

            </div>
        </div>
    </div>


    <div id=""></div>


    <!--===============================================================================================-->
    <script src="../../plugins/lovendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="../../plugins/lovendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="../../plugins/lovendor/bootstrap/js/popper.js"></script>
    <script src="../../plugins/lovendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="../../plugins/lovendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="../../plugins/lovendor/daterangepicker/moment.min.js"></script>
    <script src="../../plugins/lovendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->

    <script src="../../plugins/lovendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="../../plugins/lojs/main.js"></script>

    <script>
        $("#btnregisterJamb,#btnregisterWaec").click(function () {
         var examid = $(this).attr("examid");
            var firstName = $("#fname").val();
            if (firstName == "") { alert("Please enter User Name");
                return;

            }
                  
            var obj = {};
                obj.firstName = firstName;
                obj.examId=examid;
            var jsonData =JSON.stringify(obj);
            $.ajax({
                type: 'POST',
                contentType: 'application/json charset=utf-8',
                url: '/Account/LoginQuestion',
                data: jsonData,
                dataTyp:'json',
                success: function (response) {
                    if (response == 1) {
                        window.location = '/Index/LoadSubject';
                        //alert("You Are Welcome Here.");
                    }
                    else  alert("You Are Not Authorised Here.");
                },
                error: function (error) {
                    alert("Something Went Wrong. Kindly try again later.");
                },
                failure: function (failure) {
                    alert("Poor internet Connection.");
                }

            })


        
        })




    </script>
</body>
</html>

